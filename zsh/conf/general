# Emacs keybindings
bindkey -e

# Autocd when cannot find command
setopt autocd

# Tries to extend globs
setopt extendedglob

# Errors when does not match files
setopt nomatch

# Does not beep
unsetopt beep

# Sets Editor
export EDITOR=vim

# Sets backward/forward word
bindkey "[D" backward-word
bindkey "[C" forward-word
bindkey "^[a" beginning-of-line
bindkey "^[e" end-of-line
